Title = {{Matching-pursuit/split-operator-Fourier-transform computations of
   thermal correlation functions}},
Journal = {{J.  Chem. Phys.}},
Year = {{2005}},
Volume = {{122}},
Number = {{6}},
Month = {{FEB 8}},
Abstract = {{A rigorous and practical methodology for evaluating thermal-equilibrium
