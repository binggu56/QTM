@article{heller87,
author = {Daniel Huber and Eric J. Heller},
collaboration = {},
title = {Generalized Gaussian wave packet dynamics},
publisher = {AIP},
year = {1987},
journal = {J. Chem. Phys.},
volume = {87},
number = {9},
pages = {5302-5311},
keywords = {QUANTUM MECHANICS; GAUSSIAN PROCESSES; WAVE PACKETS; ELECTRONIC STRUCTURE; POLYATOMIC MOLECULES},
OPTURL = {http://link.aip.org/link/?JCP/87/5302/1},
OPTdoi = {10.1063/1.453647},
abstract= {We propose an extension of the semiclassical Gaussian wave packet dynamics to eliminate the three main restrictions of this method. The first restriction is that the wave packet is forced to remain Gaussian. This is correct only for quadratic, linear, or constant potentials. The second restriction is that the method is, in general, not able to treat most classically forbidden processes. The third restriction is that the norm is conserved only for Gaussian wave packets. For a superposition of Gaussians this is no longer true. We can eliminate these restrictions by an extension of the method into complex phase space, keeping time real. J. Chem. Phys. is copyrighted by The American Institute of Physics.}
}


